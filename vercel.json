{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "stallioni.com"
        }
      ],
      "destination": "https://www.stallioni.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "stallioni.vercel.app"
        }
      ],
      "destination": "https://www.stallioni.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}